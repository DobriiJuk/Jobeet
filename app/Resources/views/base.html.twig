<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome, brow!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/bootstrap/dist/css/bootstrap.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/bootstrap/dist/css/bootstrap-theme.css') }}"/>
        {#<link rel="stylesheet" href="{{ asset('css/bootstrap/docs/assets/js/raw-files.min.js') }}"/>#}
        <script type="text/javascript" src="{{ asset('js/jquery/jquery.min.js') }}"></script>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<div class="container">
    <h1>{{ block('title') }}</h1>
    {% block body %}
    {% endblock %}
    {% block content %}
    {% endblock %}

</div>
{% block javascripts %}{% endblock %}
</body>
</html>
